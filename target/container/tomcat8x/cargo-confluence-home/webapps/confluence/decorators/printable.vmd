<html>
<head>
    <title>$title - #siteTitle()</title>

    #requireResource("confluence.web.resources:master-styles")
    #standardHeader()
</head>
<body>

<div id="PageContent">
    <table border="0" cellpadding="0" cellspacing="0" width="100%">
        <tr>
            <td class="logocell">#pagetitle("spacenametitle-printable")</td>
        </tr>
    </table>

	<table class="pagecontent" border="0" cellpadding="0" cellspacing="0" width="100%">
		<tr>
			<td valign="top" class="pagebody">
				<div class="pageheader">
					<span class="pagetitle">$title</span>
				</div>

                $body
			</td>
		</tr>
	</table>
</div>

#parse ("/decorators/includes/footer.vm")
